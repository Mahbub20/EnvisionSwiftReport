	<parameter name="startTime" class="java.lang.String"/>
	<parameter name="endTime" class="java.lang.String"/>
	<parameter name="OrgId" class="java.lang.Integer"/>
	<parameter name="BusId" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 

HIS_REGISTRATION.FNAME+' '+HIS_REGISTRATION.LNAME as fullName,
HIS_REGISTRATION.DOB,
CASE 
WHEN HIS_REGISTRATION.GENDER='M' Then 'Male'
WHEN HIS_REGISTRATION.GENDER='F' Then 'Female'
ELSE 'Other'
END as Gender
,RIS_EXAM.EXAM_NAME,RIS_EXAM.EXAM_UID,
RIS_ORDERDTL.CREATED_ON as AppointmentDate,RIS_EXAM.CREATED_ON as AppointmentDate2,HIS_REGISTRATION.SSN as CitizenID,RIS_ORDERDTL.QUEUE_SERIAL,
GBL_BUS.NAME as BusName,RIS_ORDERDTL.ACCESSION_NO


FROM RIS_EXAM 
join RIS_ORDERDTL on RIS_EXAM.EXAM_ID = RIS_ORDERDTL.EXAM_ID
join RIS_ORDER on RIS_ORDERDTL.ORDER_ID = RIS_ORDER.ORDER_ID
join HIS_REGISTRATION on RIS_ORDER.REG_ID = HIS_REGISTRATION.REG_ID
join GBL_BUS on RIS_ORDERDTL.BUS_ID = GBL_BUS.ID

WHERE RIS_ORDERDTL.CREATED_ON>  $P{startTime}  and RIS_ORDERDTL.CREATED_ON< $P{endTime}  and RIS_ORDERDTL.BUS_ID=   $P{BusId}  and  RIS_ORDERDTL.ORG_ID =   $P{OrgId}  and  RIS_ORDERDTL.STATUS = 'S' ORDER BY QUEUE_SERIAL]]>
	</queryString>
